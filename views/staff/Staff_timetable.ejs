          <!-- partial -->
        </div>
     
    <!-- container-scroller -->
	
<!-- Button trigger modal -->

<!-- Modal -->
<div class="modal fade" id="TeacherModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">SELECT TEACHERS</h5>
		<input hidden id = "selected_node" value = "" >
		<input hidden id = "teacher_name" value = "" >
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body p-5">
        <div id = "teacher_area" class = "card body p-3">
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>	
<input id = "t_user_id" hidden value = <%- user.id %> ></input>
<input id = "t_user_sub_ids" hidden value = <%- user.subject %> ></input>
</div>	
	

