        <div class="main-panel">
          <!-- content-wrapper starts -->
          <div class="content-wrapper">
            <div class="row">
                  <div class="col-12 grid-margin">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title">Teacher Time Table - <%- user.cand_name %> ( <%- user.cand_id %> ) </h4>
					<!-- <button class="btn btn-success float-right" id = "load_table" >View Table</button> -->
                    <form class="form-sample d-none">
                      <div class="row">
                        <div class="col-md-6">
                          <div class="form-group row">
