<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <title>IT ADMIN</title>
    <!-- base:css -->
    <!-- <link rel="stylesheet" href="vendors/typicons/typicons.css"> -->
    <link rel="stylesheet" href="/admin-staff/css/typicons/typicons.css" />
    <!-- <link rel="stylesheet" href="vendors/css/vendor.bundle.base.css"> -->
    <link rel="stylesheet" href="/admin-staff/css/bundle.base.css" />
    <!-- endinject -->
    <!-- plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link
      rel="stylesheet"
      href="/admin-staff/css/mdi/css/materialdesignicons.min.css"
    />
    <link
      rel="stylesheet"
      href="/admin-staff/css/vertical-layout-light/style.css"
    />
    <!-- endinject -->
    <link rel="shortcut icon" href="" />
  </head>
  <body class="sidebar-dark">
    <div class="container-scroller">
      <!-- partial:partials/_navbar.html -->
      <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
        <div class="navbar-brand-wrapper d-flex justify-content-center">
          <div
            class="navbar-brand-inner-wrapper d-flex justify-content-between align-items-center w-100"
          >
            <a class="navbar-brand brand-logo" href="/admin/admin_home">
              <!-- <img src="" alt=""/> -->
            </a>
            <a class="navbar-brand brand-logo-mini" href="/admin/admin_home"
              ><img src="" alt="logo"
            /></a>
            <button
              class="navbar-toggler navbar-toggler align-self-center"
              type="button"
              data-toggle="minimize"
            >
              <span class="typcn typcn-th-menu"></span>
            </button>
          </div>
        </div>
        <div
          class="navbar-menu-wrapper d-flex align-items-center justify-content-end"
        >
          <ul class="navbar-nav mr-lg-2">
            <li class="nav-item nav-profile dropdown">
              <i class="mdi mdi-account-box"></i>
              <img src="" alt="" hidden />
              <span class="nav-profile-name">IT ADMIN</span>
            </li>
            <li class="nav-item nav-user-status dropdown">
              <!-- <p class="mb-0">Last login was 23 hours ago.</p> -->
            </li>
          </ul>
          <ul class="navbar-nav navbar-nav-right">
            <li class="nav-item nav-date dropdown">
              <a
                class="nav-link d-flex justify-content-center align-items-center"
                href="javascript:;"
              >
                <h6 class="date mb-0" id="Currentdate"></h6>
                <script>
                  document.getElementById("Currentdate").innerText =
                    new Date().toDateString();
                </script>
                <i class="typcn typcn-calendar"></i>
              </a>
            </li>
            <li class="nav-item dropdown mr-0">
              <a
                class="nav-link count-indicator dropdown-toggle d-flex align-items-center justify-content-center"
                id="notificationDropdown"
                href="#"
                data-toggle="dropdown"
              >
                <i class="typcn typcn-eject mx-0"></i>
                <!-- <span class="count"></span> -->
              </a>
              <div
                class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list"
                aria-labelledby="notificationDropdown"
              >
                <a class="dropdown-item" href="/logout">
                  <i class="typcn typcn-eject text-primary"></i>
                  Logout
                </a>
              </div>
            </li>
          </ul>
          <button
            class="navbar-toggler navbar-toggler-right d-lg-none align-self-center"
            type="button"
            data-toggle="offcanvas"
          >
            <span class="typcn typcn-th-menu"></span>
          </button>
        </div>
      </nav>
      <!-- partial -->

      <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_settings-panel.html -->
        <!-- partial -->
        <!-- partial:partials/_sidebar.html -->
        <%- include("admin_NavBar")-%>
        <!-- partial -->
        <div class="main-panel">
          <!-- content-wrapper starts -->
          <div class="content-wrapper">
            <div class="row">
              <div class="col-12 grid-margin">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title">Institute Details</h4>
                    <form class="form-sample">
                      <div class="row">
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="col-sm-3 col-form-label"
                              >Institute Name</label
                            >
                            <div class="col-sm-9">
                              <input
                                type="text"
                                class="form-control"
                                id="college_name"
                              />
                            </div>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="col-sm-3 col-form-label"
                              >Institute Place</label
                            >
                            <div class="col-sm-9">
                              <input
                                type="text"
                                class="form-control"
                                id="college_place"
                              />
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="row">
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="col-sm-3 col-form-label"
                              >Institute District</label
                            >
                            <div class="col-sm-9">
                              <input
                                type="text"
                                class="form-control"
                                placeholder=""
                                id="college_district"
                              />
                            </div>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="col-sm-3 col-form-label"
                              >Institute State</label
                            >
                            <div class="col-sm-9">
                              <select class="form-control" id="college_state">
                                <option value=""></option>
                                <% if(state.length!=0){ var i=1;
                                state.forEach(function(state){ %>
                                <option value="<%=state.name.toUpperCase() %>">
                                  <%=state.name.toUpperCase() %>
                                </option>
                                <% i++; }) %> <% } else{ %> <% } %>
                              </select>
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="row">
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="col-sm-3 col-form-label"
                              >Pin Code</label
                            >
                            <div class="col-sm-9">
                              <input
                                type="number"
                                class="form-control"
                                id="college_pin"
                              />
                            </div>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group row">
                            <label class="col-sm-3 col-form-label"
                              >Phone Number</label
                            >
                            <div class="col-sm-9">
                              <input
                                type="number"
                                class="form-control"
                                id="college_phone"
                              />
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="row">
                        <div class="col-md-12">
                          <div class="form-group row">
                            <label class="col-sm-2 col-form-label">
                              Institute E-Mail</label
                            >
                            <div class="col-sm-10">
                              <input
                                type="email"
                                class="form-control"
                                id="college_mail"
                              />
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="form-group">
                        <label>Institute Logo</label>
                        <input
                          type="file"
                          name="img[]"
                          class="file-upload-default"
                          accept=".png,.jpg,.jpeg"
                          id="logo"
                        />
                        <div class="input-group col-xs-12">
                          <input
                            type="text"
                            class="form-control file-upload-info"
                            disabled=""
                            placeholder="Upload Image"
                            id="college_t_logo"
                          />
                          <span class="input-group-append">
                            <button
                              class="file-upload-browse btn btn-dark btn-icon-text"
                              type="button"
                            >
                              <i
                                class="typcn typcn-upload btn-icon-prepend"
                              ></i>
                              Upload
                            </button>
                          </span>
                        </div>
                      </div>
                      <div class="row">
                        <div class="col-md-8">
                          <p id="displaymsg"></p>
                        </div>
                        <div class="col-md-4">
                          <div class="form-group" style="float: right">
                            <button
                              type="reset"
                              class="btn btn-warning btn-icon-text"
                            >
                              <i
                                class="typcn typcn-refresh-outline btn-icon-prepend"
                              ></i>
                              Reset
                            </button>
                            <button
                              type="button"
                              class="btn btn-primary btn-icon-text"
                              onclick="submitted('logo')"
                            >
                              <i
                                class="typcn typcn-document btn-icon-prepend"
                              ></i>
                              Submit
                            </button>
                          </div>
                        </div>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!-- content-wrapper ends -->
          <!-- partial:partials/_footer.html -->
          <footer class="footer">
            <div class="card">
              <div class="card-body">
                <div
                  class="d-sm-flex justify-content-center justify-content-sm-between"
                >
                  <span
                    class="text-muted text-center text-sm-left d-block d-sm-inline-block"
                    >Copyright &copy; 2021
                    <a href="https://www.ashiantechnologies.com"
                      >Ashian Technologies</a
                    >. All rights reserved.</span
                  >
                </div>
              </div>
            </div>
          </footer>
          <!-- partial -->
        </div>
        <!-- main-panel ends -->
      </div>
      <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->

    <!-- base:js -->
    <script src="/admin-staff/js/devs/bundle.base.js"></script>

    <script src="/admin-staff/js/devs/off-canvas.js"></script>
    <script src="/admin-staff/js/devs/hoverable-collapse.js"></script>
    <script src="/admin-staff/js/devs/template.js"></script>
    <!-- Custom js for this page-->
    <script src="/admin-staff/js/devs/dashboard.js"></script>
    <script src="/admin-staff/js/devs/file-upload.js"></script>
    <!-- End custom js for this page-->
    <script defer src="/admin-staff/js/api/college_details.js"></script>
    <script>
      document.getElementById("college_name").value =
        "<%-college_details[0].college_name%>";
      document.getElementById("college_place").value =
        "<%-college_details[0].college_place%>";
      document.getElementById("college_district").value =
        "<%-college_details[0].college_district%>";
      document.getElementById("college_state").value =
        "<%-college_details[0].college_state%>";
      document.getElementById("college_pin").value =
        "<%-college_details[0].college_pin%>";
      document.getElementById("college_phone").value =
        "<%-college_details[0].college_phone%>";
      document.getElementById("college_mail").value =
        "<%-college_details[0].college_mail%>";
      document.getElementById("college_t_logo").value =
        "<%-college_details[0].college_t_logo%>";
    </script>
  </body>
</html>
